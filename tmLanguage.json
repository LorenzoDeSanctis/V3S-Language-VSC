{
  "scopeName": "source.v3s",
  "name": "Visual 3D Pipeline Script",
  "fileTypes": ["v3s"],
  "patterns": [
    {
      "name": "comment.line.exclamation.v3s",
      "match": "!.*$"
    },
    {
      "name": "entity.name.function.v3s",
      "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
    },
    {
      "name": "keyword.parameter.v3s",
      "match": "/[A-Z_]+="
    },
    {
      "name": "punctuation.terminator.v3s",
      "match": ";"
    }
  ],
  "repository": {},
  "scope": "source.v3s"
}
